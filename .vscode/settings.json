{
  "python.defaultInterpreterPath": "/opt/miniconda3/envs/kyopro/bin/python",
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "python" : "oj test -c '$pythonPath $fullFileName' -d $dir/tests -S",
    "typescript" : "oj test -c 'ts-node $fullFileName' -d $dir/tests",
    "cpp" : "cd $dir && g++ $fileName -o main && oj test -c $dir/main -d $dir/tests",
  },
  "files.associations": {
    "iostream": "cpp",
    "cstddef": "cpp"
  }
}